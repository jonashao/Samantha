<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <!-- Customize your theme here. -->
        <!--<item name="colorPrimary">@color/white</item>-->
        <!--<item name="colorPrimaryDark">@color/white_2</item>-->
        <!--<item name="colorAccent">@color/colorAccent</item>-->

    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        
        <item name="android:titleTextStyle">@style/AppTheme.TitleTextStyle</item>
    </style>

    <style name="AppTheme.TitleTextStyle">
        <item name="android:textColor">@color/green_dark</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.ActionBar"/>

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>

    <style name="AppTheme.SwipeMenu">
        <item name="android:gravity">center</item>
        <item name="android:clickable">true</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:color">@android:color/black</item>
    </style>



</resources>
